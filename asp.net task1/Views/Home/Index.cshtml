


<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Surname</th>
            <th>Age</th>
            <th>Country</th>
            <th>PhoneNumber</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@ViewBag.Name</td>
            <td>@ViewBag.Surname</td>
            <td>@ViewBag.Age</td>
            <td>@ViewBag.Country</td>
            <td>@ViewBag.PhoneNumber</td>
        </tr>
    </tbody>
</table>